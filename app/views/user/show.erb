<h2>Welcome to Your Question Page!</h2>
<p>Username: <%=@user.username %></p>
<% if is_current_user? %>
<p>Email: <%=@user.email%></p>
<% end %>

<%unless @user.questions.empty? %>
  <h3>Here are all of the questions you have asked:</h3>


  <ul> <%@user.questions.each do |question| %>
    <li class= <%="#{question.id}"%>>
      <a href= <%= "/questions/#{question.id}" %>><%= question.title%></a>
      <a class="delete" href=<%= "/question/#{question.id}"%>>delete</a>
      <!-- create display for votes -->
    </li>
    <% end %>
  </ul>
<% end %>

